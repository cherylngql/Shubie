{
  "name": "shubie",
  "version": "1.0.0",
  "description": "An interactive dog living on your desktop!",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "start-dev": "webpack && electron .",
    "build-mas"     : "electron-packager . shubie --platform=mas --icon Icons/shubie.icns --overwrite",
    "build-win32"   : "electron-packager . shubie --platform=win32 --icon Icons/shubie.ico --overwrite",
    "build"         : "npm run build-mas && npm run build-win32"
  },
  "author": "Cheryl",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "electromon": "^1.0.10",
    "gsap": "^2.0.1",
    "p2": "^0.7.1",
    "phaser": "^3.11.0",
    "pixi": "^0.3.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0"
  },
  "devDependencies": {
    "electron": "^2.0.6",
    "electron-packager": "^12.1.0"
  }
}
